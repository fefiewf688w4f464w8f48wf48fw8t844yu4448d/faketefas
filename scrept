--Disable Cube Of Death

function KillAura()
    spawn(function()
        while getgenv().KillAura == true do
            wait()
            for i, v in pairs(workspace:GetChildren()) do
                if game.Players.LocalPlayer.Character then
                    if game.Players.LocalPlayer.Character:FindFirstChild("Torso") then
                        if v.ClassName == "Model" then
                            if v:FindFirstChild("Torso") and v.Name ~= game.Players.LocalPlayer.Name then
                                if (v.Torso.Position - game.Players.LocalPlayer.Character.Torso.Position).magnitude < getgenv().Reach then
                                    Glove_Remotes[game.Players.LocalPlayer.leaderstats.Glove.Value]:FireServer(v:FindFirstChild("Torso"))
                                end
                            end
                        end
                    end
                end
            end
        end
    end)
end
spawn(function()
    local mt = getrawmetatable(game)
    local oldMeta = mt.__namecall
    setreadonly(mt, false)
    game:GetService("UserInputService").InputBegan:Connect(function(a, b)
        if not b then
            if a.UserInputType == Enum.UserInputType.MouseButton1 and getgenv().SilentAim == true then
                Glove_Remotes[game.Players.LocalPlayer.leaderstats.Glove.Value]:FireServer(game.Players.LocalPlayer.Character.Head)
            end
        end
    end)
    mt.__namecall = function(self, ...)
        local args = {...}
        local method = getnamecallmethod()
        if method == "FireServer" and self == Glove_Remotes[game.Players.LocalPlayer.leaderstats.Glove.Value] and getgenv().SilentAim == true then
            local target = nil
            for i, v in pairs(workspace:GetChildren()) do
                if game.Players.LocalPlayer.Character then
                    if v.ClassName == "Model" and v:FindFirstChild("Head") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name and (v.Head.Position - game.Players.LocalPlayer.Character.Head.Position).magnitude < 15 then
                        target = v
                    end
                end
            end
            if target ~= nil then
                args[1] = target:FindFirstChild("Torso")
            end
            return self.FireServer(self, unpack(args))
        end
        return oldMeta(self, ...)
    end
end)

local function Fly()
    spawn(function()
        local UIS = game:GetService("UserInputService")

        game:GetService("RunService").RenderStepped:Connect(function()
            if getgenv().Fly == true then
                if game.Players.LocalPlayer.Character then
                    game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
                    local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(game.Workspace.CurrentCamera.CFrame.LookVector.X, game.Workspace.CurrentCamera.CFrame.LookVector.Y, game.Workspace.CurrentCamera.CFrame.LookVector.Z)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, pos)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.BodyGyro.CFrame = workspace.CurrentCamera.CoordinateFrame
                end
            end
            if UIS:IsKeyDown(Enum.KeyCode.W) and getgenv().Fly == true then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -getgenv().FlySpeed) end
            if UIS:IsKeyDown(Enum.KeyCode.S) and getgenv().Fly == true then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, getgenv().FlySpeed) end
            if UIS:IsKeyDown(Enum.KeyCode.D) and getgenv().Fly == true then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(getgenv().FlySpeed, 0, 0) end
            if UIS:IsKeyDown(Enum.KeyCode.A) and getgenv().Fly == true then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(-getgenv().FlySpeed, 0, 0) end
            if UIS:IsKeyDown(Enum.KeyCode.E) and getgenv().Fly == true then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, getgenv().FlySpeed, 0) end
            if UIS:IsKeyDown(Enum.KeyCode.Q) and getgenv().Fly == true then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, -getgenv().FlySpeed, 0) end
        end)
    end)
end
Fly()
function NoRagdoll()
    spawn(function()
        while getgenv().NoRagdoll == true do
            wait()
            if game.Players.LocalPlayer.Character then
                if game.Players.LocalPlayer.Character:FindFirstChild("Ragdolled") then
                    if game.Players.LocalPlayer.Character:FindFirstChild("Ragdolled").Value == true then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
                    else
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
                    end
                end
            end
        end
    end)
end
function AutoSlapples()
    spawn(function()
        while getgenv().AutoSlapples == true do
            wait(0.2)
            for i, v in next, workspace.Arena.island5.Slapples:GetDescendants() do
                if v.ClassName == "TouchTransmitter" then
                    if game.Players.LocalPlayer.Character then
                        if game.Players.LocalPlayer.Character:FindFirstChild("Head") then
                            firetouchinterest(game.Players.LocalPlayer.Character.Head, v.Parent, 0)
                            firetouchinterest(game.Players.LocalPlayer.Character.Head, v.Parent, 1)
                        end
                    end
                end
            end
        end
    end)
end
function AutoTycoonGlove()
    spawn(function()
        while getgenv().AutoTycoonGlove == true do
            wait()
            if game.Players.LocalPlayer.Character then
                if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Arena.Plate.CFrame
                end
            end
        end
    end)
end
function Speed()
    while getgenv().TpSpeed == true do
        wait()
        if game.Players.LocalPlayer.Character then
            if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                if game.Players.LocalPlayer.Character.Humanoid.MoveDirection ~= Vector3.new(0, 0, 0) then
                    game.Players.LocalPlayer.Character:TranslateBy(Vector3.new(game.Players.LocalPlayer.Character.Humanoid.MoveDirection.X * getgenv().TpSpeedSpeed, game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Y * getgenv().TpSpeedSpeed, game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Z * getgenv().TpSpeedSpeed))
                end
            end
        end
    end
end
function GoldenGodMode()
    spawn(function()
        while getgenv().GodMode == true and getgenv().GoldenMethod == true do
            wait()
            if game:GetService("Players").LocalPlayer.Character:FindFirstChild("entered") ~= nil then
                if game.Players.LocalPlayer.Character then
                    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        if game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Color ~= Color3.fromRGB(255, 255, 0) then
                            game:GetService("ReplicatedStorage"):FindFirstChild("Goldify"):FireServer(true);
                        end
                    end
                end
            end
        end
    end)
end
function UnoReverseMethod()
    spawn(function()
        while getgenv().GodMode == true and getgenv().UnoReverseMethod == true do
            wait(5.5)
            if game:GetService("Players").LocalPlayer.Character:FindFirstChild("entered") ~= nil then
                if game.Players.LocalPlayer.Character then
                    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        if game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Color ~= Color3.fromRGB(255, 255, 0) then
                            game:GetService("ReplicatedStorage"):FindFirstChild("ReverseAbility"):FireServer()
                        end
                    end
                end
            end
        end
    end)
end
function AntiTimeStop()
    spawn(function()
        while getgenv().AntiTimeStop == true do
            wait()
            if game.Players.LocalPlayer.Character then
                if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    if game.Players.LocalPlayer.Character:FindFirstChild("Ragdolled").Value == false then
                        game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Anchored = false
                        if game.Players.LocalPlayer.Character:FindFirstChild("Head") then
                            game.Players.LocalPlayer.Character:FindFirstChild("Head").Anchored = false
                        end
                    end
                end
            end
        end
    end)
end
function HideUnoReverseEffect()
    spawn(function()
        while getgenv().HideUnoReverseEffect == true do
            wait()
            if game.Players.LocalPlayer.Character then
                if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                        if v.ClassName == "SelectionBox" or v.Name == "UnoReverseCard" then
                            v:Destroy()
                        end
                    end
                end
            end
        end
    end)
end
function AntiMail()
    spawn(function()
        while getgenv().AntiMail == true do
            wait()
            if game.Players.LocalPlayer.PlayerGui:FindFirstChild("MailPopup") then
                game.Players.LocalPlayer.PlayerGui:FindFirstChild("MailPopup"):Destroy()
            end
        end
    end)
end
function Rotations()
    spawn(function()
        while getgenv().Rotations == true do
            wait()
            if game.Players.LocalPlayer.Character then
                if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    for i, v in pairs(workspace:GetChildren()) do
                        if v.ClassName == "Model" and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name and (v:FindFirstChild("HumanoidRootPart").Position - game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position).magnitude < getgenv().Reach then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.lookAt(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, Vector3.new(v.HumanoidRootPart.Position.X, game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y, v.HumanoidRootPart.Position.Z))
                            game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
                        else
                            game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
                        end
                    end
                end
            end
        end
    end)
end

function AntiBob()
    spawn(function()
        while getgenv().AntiBob == true do
            wait()
            for i, v in pairs(workspace:GetChildren()) do
                if v.ClassName == "StringValue" and v.Name == "Target" and v.Parent.ClassName == "Model" and v.Value == game.Players.LocalPlayer.Name then
                    v.HumanoidRootPart.CFrame = game:GetService("Workspace").Arena.CubeOfDeathArea["the cube of death(i heard it kills)"].CFrame
                end
            end
        end
    end)
end
